    function hideIETip() {
        $(".top-notification").hide();
    }
    $('document').ready(function () {
        if ($.isIE() && parseInt(window.browser['version']) < 10) {
            $(".top-notification").show();
        }
        var mySwiper = new Swiper('.swiper-container', {
            autoplay: 4000, //可选选项，自动滑动
            pagination: '.bullet',
            paginationClickable: true,//点击分页器后会控制swiper切换
            autoplayDisableOnInteraction: false,
            loop: true,
            slideSpeed: 1800,
        });
        $('.index-xia a').hover(function () {
            $('dl', this).hide();
            $('.d1', this).show();
        }, function () {
            $('dl', this).show();
            $('.d1', this).hide();
        });

        $('.hover-a').hover(function () {
            $('.hover-ma').show();
        }, function () {
            $('.hover-ma').hide();
        })
    })
</script>
