<script>
    $('document').ready(function () {
        //获得当前浏览器高度
        var height = $(window).height();
        $(".index-body-top").css("height", height);
        // team轮播
        var galleryThumbs = new Swiper('.gallery-thumbs', {
            spaceBetween: 10,
            slidesPerView: 4,
            touchRatio: 0.2,
            loop: true,
            loopedSlides: 5, //looped slides should be the same
            slideToClickedSlide: true,
            nextButton: '.swiper-button-next',
            prevButton: '.swiper-button-prev',
        });

        $('.div-rel').hover(function () {
            $('.p-pos', this).stop().fadeOut(800);
            $("img", this).stop().animate({'opacity': '1'}, 800)
        }, function () {
            $('.p-pos', this).stop().fadeIn(800);
            $("img", this).stop().animate({'opacity': '.4'}, 800)
        })
       /* // team的文字hover效果
        $('.btn-banner .swiper-wrapper div').hover(function () {
            $(this).children('span').stop().fadeIn(1500).css('
        }, function () {
            $(this).children('span').stop().fadeOut();
        });*/
        // case opacity
        $('.case-img').hover(function () {
            //$(this).addClass('rgba-color');
            $('.case-text', this).stop().fadeOut(800);
        }, function () {
            //$(this).removeClass('rgba-color');
            $('.case-text', this).stop().fadeIn(800);
        });
        // 元素的文字下滑效果
        $('.yuansu-img a').hover(function () {
            $('span', this).stop().fadeIn(800);
            $(this).css({'background-color': 'rgba(0,0,0,1.0)', 'opacity': '1.0'});
        }, function () {
            $('span', this).stop().fadeOut(800);
            $(this).css({'background-color': 'rgba(0,0,0,.5)', 'opacity': '0.5'});
        });

        //锚链接
        $(".icon-xia,#liys").click(function (event) {
            $(window).scrollTop(height);
        });
        $("#litd").click(function (event) {
            $(window).scrollTop(height + $('.xia-pop').height() + $('.jieshao').height() + 150);
        });
        $("#lial").click(function (event) {
            $(window).scrollTop($(document).height());
        });


        var mapto = window.location.href.getQueryString("mapto");
        if (mapto != null) {
            $("#" + mapto).trigger('click');
        }
    })
</script>

